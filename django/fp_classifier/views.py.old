def about(request):
    f = open('/home/dev/github/my-repos/finna-be-ninja/www/about/index.html','r')
    template = f.read()
    fa = open('/home/dev/github/my-repos/finna-be-ninja/www/about/about.html','r')
    insert = fa.read()
