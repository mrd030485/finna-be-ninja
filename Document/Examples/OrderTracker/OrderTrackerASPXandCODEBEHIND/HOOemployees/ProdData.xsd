<?xml version="1.0" standalone="yes"?>
<xs:schema id="ProdData" targetNamespace="http://www.tempuri.org/ProdData.xsd" xmlns:mstns="http://www.tempuri.org/ProdData.xsd" xmlns="http://www.tempuri.org/ProdData.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="ProdData" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Orders">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="orderNumber" type="xs:int" />
              <xs:element name="dueDate" type="xs:dateTime" minOccurs="0" />
              <xs:element name="orderDate" type="xs:dateTime" />
              <xs:element name="artDueDate" type="xs:dateTime" minOccurs="0" />
              <xs:element name="rushReorderRegular" type="xs:string" />
              <xs:element name="customerNumber" type="xs:string" />
              <xs:element name="numColorsFront" type="xs:int" minOccurs="0" />
              <xs:element name="numColorsBack" type="xs:int" minOccurs="0" />
              <xs:element name="numColorsSleeve" type="xs:int" minOccurs="0" />
              <xs:element name="artFinished" type="xs:boolean" minOccurs="0" />
              <xs:element name="productionFinished" type="xs:boolean" minOccurs="0" />
              <xs:element name="orderFinished" type="xs:boolean" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:Orders" />
      <xs:field xpath="mstns:orderNumber" />
    </xs:unique>
  </xs:element>
</xs:schema>